{
    "deno.enablePaths": ["supabase/functions"],
    "deno.lint": true,
    "deno.unstable": [
        "bare-node-builtins",
        "byonm",
        "sloppy-imports",
        "unsafe-proto",
        "webgpu",
        "broadcast-channel",
        "worker-options",
        "cron",
        "kv",
        "ffi",
        "fs",
        "http",
        "net"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "files.exclude": {
        "**/*.rpyc": true,
        "**/*.rpa": true,
        "**/*.rpymc": true,
        "**/cache/": true
    }
}
